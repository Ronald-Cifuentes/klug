{
  "main": "index.js",
  "repository": "git@github.p.com:Ronald-Cifuentes/vite-boilerplate.git",
  "author": "Ronald Cifuentes <ronald.cifuentes2020@gmail.com>",
  "license": "MIT",
  "name": "vite-boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "critical": "critical dist/index.html --base=dist --inline --width=1300 --height=900",
    "lint": "eslint src --ext ts,tsx",
    "preview": "vite preview",
    "commit": "npx gitmoji-cli -c",
    "gen": "npx generate-react-cli c",
    "format": "prettier --write .",
    "test": "npx jest --clearCache && jest",
    "test:watch": "jest --watch",
    "test:ci": "npm run lint && npm run format && npm run test",
    "cov": "npm run test \"$src\" -- --coverage --collectCoverageFrom=\"$(echo $src | sed 's/.spec//')\"",
    "prepare": "husky install",
    "ts-check": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "typescript": "5.8.3",
    "vite": "6.3.5"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/react": "19.1.6",
    "@types/react-dom": "19.1.5",
    "@typescript-eslint/eslint-plugin": "8.33.0",
    "@typescript-eslint/parser": "8.33.0",
    "@vitejs/plugin-react": "4.5.0",
    "commitlint-config-gitmoji": "2.3.1",
    "critical": "7.2.1",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-jest": "28.11.1",
    "eslint-plugin-prettier": "5.4.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-transform-stub": "2.0.0",
    "prettier": "3.5.3",
    "rollup-plugin-critical": "1.0.15",
    "rollup-plugin-visualizer": "6.0.0",
    "sass-embedded": "1.89.0",
    "ts-jest": "29.3.4",
    "ts-node": "10.9.2",
    "vite-plugin-compression": "0.5.1"
  },
  "packageManager": "yarn@4.9.1",
  "engines": {
    "node": "22.13.1"
  }
}
